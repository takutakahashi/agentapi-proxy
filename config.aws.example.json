{
  "auth": {
    "enabled": true,
    "aws": {
      "enabled": true,
      "region": "ap-northeast-1",
      "allowed_account_ids": ["123456789012", "987654321098"],
      "team_tag_key": "Team",
      "required_tag_key": "agentapi-proxy",
      "required_tag_value": "enabled",
      "cache_ttl": "1h",
      "user_mapping": {
        "default_role": "guest",
        "default_permissions": ["read"],
        "team_role_mapping": {
          "platform": {
            "role": "admin",
            "permissions": ["*"],
            "env_file": "/etc/agentapi/envs/platform.env"
          },
          "backend": {
            "role": "developer",
            "permissions": ["read", "write", "execute", "debug"]
          },
          "frontend": {
            "role": "developer",
            "permissions": ["read", "write", "execute"]
          },
          "data-science": {
            "role": "member",
            "permissions": ["read", "write"]
          }
        }
      }
    }
  }
}
